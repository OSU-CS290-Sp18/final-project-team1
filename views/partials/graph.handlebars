<div class="graph-container">
  {{#each graphs}}
    <div class="graph">
      <div class="graph-description">
        <p>{{description}}</p>
      </div>
      <div class="graph-area">
        <div style="background-color: green; width: {{percent}}%; height: 2rem;"></div>
        <p class="graph-percent">{{percent}}%</p>
      </div>
    </div>
  {{/each}}
</div>
